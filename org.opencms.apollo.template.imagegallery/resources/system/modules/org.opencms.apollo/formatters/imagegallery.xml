<?xml version="1.0" encoding="UTF-8"?>

<NewFormatters xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="opencms://system/modules/org.opencms.ade.config/schemas/formatters/new_formatter.xsd">
  <NewFormatter language="en">
    <NiceName><![CDATA[%(key.formatter.a-imagegallery.default)]]></NiceName>
    <Type><![CDATA[a-imagegallery]]></Type>
    <Jsp>
      <link type="WEAK">
        <target><![CDATA[/system/modules/org.opencms.apollo/formatters/imagegallery.jsp]]></target>
        <uuid>68958b5e-8f73-11e5-9ae7-0242ac11002b</uuid>
      </link>
    </Jsp>
    <Rank><![CDATA[1000]]></Rank>
    <Match>
      <Types>
        <ContainerType><![CDATA[imagegallery]]></ContainerType>
        <ContainerType><![CDATA[element]]></ContainerType>
      </Types>
    </Match>
    <Preview>false</Preview>
    <SearchContent>true</SearchContent>
    <AutoEnabled>true</AutoEnabled>
    <Detail>true</Detail>
    <NestedContainers>false</NestedContainers>
    <Setting>
      <PropertyName><![CDATA[hidetitle]]></PropertyName>
      <DisplayName><![CDATA[%(key.apollo.setting.hidetitle)]]></DisplayName>
      <Widget><![CDATA[checkbox]]></Widget>
      <Default><![CDATA[false]]></Default>
    </Setting>
    <Setting>
      <PropertyName><![CDATA[wrapperclass]]></PropertyName>
      <DisplayName><![CDATA[%(key.apollo.setting.cssclass)]]></DisplayName>
      <Description><![CDATA[%(key.apollo.setting.cssclass.description)]]></Description>
      <Widget><![CDATA[String]]></Widget>
    </Setting>
    <Setting>
      <PropertyName><![CDATA[cssClass]]></PropertyName>
      <DisplayName><![CDATA[%(key.apollo.setting.imagegallery.css)]]></DisplayName>
      <Description><![CDATA[%(key.apollo.setting.imagecss.description)]]></Description>
      <Widget><![CDATA[selectcombo]]></Widget>
      <Default><![CDATA[square-sm-6 square-md-4 square-lg-3]]></Default>
      <WidgetConfig><![CDATA[
        square-xs-6 square-sm-4 square-md-3 square-lg-2:%(key.apollo.setting.imagegallery.css.xs6-sm4-md3-lg2)|
        square-xs-6 square-sm-4 square-lg-2:            %(key.apollo.setting.imagegallery.css.xs6-sm4-lg2)|
        square-sm-6 square-md-4 square-lg-3:            %(key.apollo.setting.imagegallery.css.sm6-md4-lg3)|
        square-sm-6 square-lg-4:                        %(key.apollo.setting.imagegallery.css.sm6-lg4)|
        square-md-6:                                    %(key.apollo.setting.imagegallery.css.md6)|
        square-sm-6:                                    %(key.apollo.setting.imagegallery.css.sm6)|
        square-xs-12:                                   %(key.apollo.setting.imagegallery.css.xs12)
        ]]></WidgetConfig>
    </Setting>
    <Setting>
      <PropertyName><![CDATA[imagesPerPage]]></PropertyName>
      <DisplayName><![CDATA[%(key.apollo.setting.imagesperpage)]]></DisplayName>
      <Description><![CDATA[%(key.apollo.setting.imagesperpage.description)]]></Description>
      <Widget><![CDATA[string]]></Widget>
      <Default><![CDATA[12]]></Default>
      <RuleRegex><![CDATA[[0-9]+]]></RuleRegex>
      <RuleType><![CDATA[error]]></RuleType>
      <Error><![CDATA[%(key.apollo.setting.imagesperpage.error)]]></Error>
    </Setting>
    <Setting>
      <PropertyName><![CDATA[showTitle]]></PropertyName>
      <DisplayName><![CDATA[%(key.apollo.setting.showtitle)]]></DisplayName>
      <Description><![CDATA[%(key.apollo.setting.showtitle.description)]]></Description>
      <Widget><![CDATA[checkbox]]></Widget>
      <Default><![CDATA[true]]></Default>
    </Setting>
    <Setting>
      <PropertyName><![CDATA[showCopyright]]></PropertyName>
      <DisplayName><![CDATA[%(key.apollo.setting.image.text.showcopyright)]]></DisplayName>
      <Widget><![CDATA[checkbox]]></Widget>
      <Default><![CDATA[true]]></Default>
    </Setting>
    <Setting>
      <PropertyName><![CDATA[autoload]]></PropertyName>
      <DisplayName><![CDATA[%(key.apollo.setting.autoload)]]></DisplayName>
      <Widget><![CDATA[checkbox]]></Widget>
      <Default><![CDATA[true]]></Default>
    </Setting>
  </NewFormatter>
</NewFormatters>
